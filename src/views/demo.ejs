  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.0/dist/js/bootstrap.bundle.min.js"></script>
  <style>
    .container {
      margin-top: 100px;
    }

    .newarrival {
      background: chocolate;
      width: 50px;
      color: whitesmoke;
      font-size: 12px;
      font-weight: bold;
    }

    .col-md-7 h2 {
      color: tomato;
    }

    .stars {
      height: 15px;
    }

    .price {
      color: #FE980F;
      font-size: 15px;
      font-weight: bold;
      padding-top: 30px;
    }

    input {
      border: 2px solid black;
      font-weight: bold;
      height: 33px;
      text-align: center;
      width: 30px;
    }

    .cart {
      background: #FE980F;
      color: #FFFFFF;
      font-size: 15px;
      margin-left: 20px;
    }

    .para {
      margin-left: 15px;
    }
  </style>
</head>

